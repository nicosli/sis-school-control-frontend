    "lodash": "^4.17.4",
    "vue": "^2.5.7",
    "jquery": "^3.2",
    "vue-resource": "^1.0.3",
    "moment": "^2.15.2",
    "popper.js": "^1.12.9",
    "vue-router": "^3.0.1",
    "axios": "^0.17.1",
